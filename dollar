#!/bin/bash
printf "\$\$ shell's pid is %s\n" "$$"
printf "\$! the shell's last background pid  %s\n" "$!"
printf "The \$? commandline's error code %s\n" "$?"
printf "The \$* is all parameters %s\n" "$*"
printf "The \$@ is all paremeters %s\n" "$@"
printf "The \$# is the number of parameters %s\n" "$#"
printf "The \$0 is %s\n" "$0"
printf "The \$1 is %s\n" "$1"
printf "The \$2 is %s\n" "$2"
